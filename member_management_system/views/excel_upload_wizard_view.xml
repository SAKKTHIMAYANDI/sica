<odoo>
    <record id="view_import_movie_list_wizard" model="ir.ui.view">
        <field name="name">movie.excel.upload.wizard</field>
        <field name="model">movie.excel.upload.wizard</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="excel_file"/>
                    <field name="file_name" readonly="1"/>
                </group>
                <footer>
                    <button string="Import" type="object" name="action_import" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="action_import_movie_list" model="ir.actions.act_window">
        <field name="name">Import Movie List</field>
        <field name="res_model">movie.excel.upload.wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>
</odoo>

<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="expense_type_tree_view" model="ir.ui.view">
            <field name="name">expense.type.tree</field>
            <field name="model">expense.type</field>
            <field name="arch" type="xml">
                <tree string="Expense Type" editable="bottom">
                    <field name="name" string="Expense Name" required="1"/>
                </tree>
            </field>
        </record>


        <record id="action_expense_type" model="ir.actions.act_window">
            <field name="name">Expense Type</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">expense.type</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Click on Create button to create expense.
                </p>
            </field>
        </record>

            <menuitem id="menu_expense_type" name="Expense Type" parent="member_management_system.menu_expense_management" action="action_expense_type" sequence="10"/>
</odoo>